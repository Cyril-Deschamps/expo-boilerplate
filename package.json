{
  "name": "@cyril-deschamps/expo-boilerplate",
  "version": "1.0.1",
  "author": "Cyril Deschamps <contact@cyrildeschamps.fr>",
  "description": "Best starter for Expo",
  "license": "MIT",
  "homepage": "https://github.com/Cyril-Deschamps/expo-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cyril-Deschamps/expo-boilerplate.git"
  },
  "keywords": [
    "expo",
    "boilerplate",
    "typescript",
    "react",
    "native",
    "react-native",
    "eas"
  ],
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint-check:react": "cross-env NODE_ENV=production && eslint src .eslintrc.js --max-warnings 0 --ext .tsx,.js,.ts",
    "lint-check:typing": "tsc --noEmit",
    "lint-check:prettier": "prettier --check src",
    "lint-fix:react": "cross-env NODE_ENV=production && eslint src .eslintrc.js --max-warnings 0 --ext .tsx,.js,.ts --fix",
    "lint-fix:prettier": "prettier --write src"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "axios": "^1.2.1",
    "date-fns": "^2.29.3",
    "expo": "~47.0.8",
    "expo-asset": "~8.6.2",
    "expo-constants": "~14.0.2",
    "expo-font": "~11.0.1",
    "expo-linking": "~3.2.3",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-system-ui": "~2.0.1",
    "expo-web-browser": "^12.0.0",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-dotenv": "^3.4.6",
    "react-native-root-toast": "^3.3.1",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^26.6.3",
    "jest-expo": "~47.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "jest-expo"
  }
}
